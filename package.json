{
  "name": "Musicoin-wallet",
  "version": "1.0.1",
  "private": true,
  "node-main": "./hub/msc.js",
  "main": "./interface/index.html",
  "window": {
    "frame": true,
    "toolbar": true,
    "position": "center",
    "width": 1000,
    "height": 600
  },
  "webkit": {
    "plugin": true,
    "page-cache": true
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "ethereum-blockies": "github:ethereum/blockies",
    "ethers": "^3.0.7",
    "fs-extra": "^5.0.0",
    "fs-finder": "^1.8.1",
    "jsqr": "^1.0.4",
    "ntp-client": "^0.5.3",
    "pngjs": "^3.3.2",
    "qrcode": "^1.2.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "web3": "^0.20.6",
    "y18n": "^4.0.0",
    "zxcvbn": "^4.4.2"
  },
  "scripts": {
    "preinstall": "cd interface/ ; yarn ; cd ..",
    "install": "nwb nwbuild -o ./build",
    "postinstall": "git clone https://github.com/Musicoin/go-musicoin.git; yarn run build-gmc",
    "build-gmc": "cd go-musicoin; build/env.sh go run build/ci.go install ../build/gmc; cd .."
  },
  "snyk": true,
  "devDependencies": {
    "nw": "^0.29.3",
    "nwjs": "^1.4.4",
    "nwjs-builder": "^1.14.0",
    "snyk": "^1.70.3"
  }
}
