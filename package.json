    "height": 600
  },
  "webkit": {
    "plugin": true,
    "page-cache": true
  },
  "dependencies": {
    "@musicoin/gmc": "github:etaletai13/gmc",
    "bluebird": "^3.5.1",
    "ethereum-blockies": "github:ethereum/blockies",
    "ethers": "^3.0.7",
    "fs-extra": "^5.0.0",
    "fs-finder": "^1.8.1",
    "jsqr": "^1.0.4",
    "ntp-client": "^0.5.3",
    "pngjs": "^3.3.2",
    "qrcode": "^1.2.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "web3": "^0.20.6",
    "y18n": "^4.0.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "bower": "^1.8.4",
    "nwjs-builder": "^1.14.0",
    "snyk": "^1.71.0"
  },
  "scripts": {
    "build:app": "rm -rf Musicoin** && nwb nwbuild",
    "build:interface": "cd interface && bower install && cd ..",
    "postinstall": "mkdir -p bin/gmc && cp node_modules/\\@musicoin/gmc/bin/gmc bin/gmc/ && yarn build:interface && yarn build:app",
