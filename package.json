{
  "name": "musicoin-wallet",
  "installationMethod": "unknown",
  "version": "1.5.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Musicoin/desktop.git"
  },
  "node-main": "./hub/msc.js",
  "main": "./interface/index.html",
  "bugs": "https://github.com/Musicoin/desktop/issues",
  "window": {
    "frame": true,
    "toolbar": true,
    "position": "center",
    "width": 1000,
    "height": 600
  },
  "webkit": {
    "plugin": true,
    "page-cache": true
  },
  "engines": {
    "node": ">=8.0.0",
    "yarn": ">=1.5.0"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "ethereum-blockies": "github:ethereum/blockies",
    "ethers": "3.0.7",
    "fs-extra": "5.0.0",
    "fs-finder": "1.8.1",
    "jsqr": "1.0.4",
    "ntp-client": "0.5.3",
    "nwjs-builder": "1.14.0",
    "pngjs": "3.3.2",
    "qrcode": "1.2.0",
    "request": "2.85.0",
    "request-promise-native": "1.0.5",
    "web3": "github:etaletai13/web3.js",
    "y18n": "4.0.0",
    "zxcvbn": "4.4.2"
  },
  "scripts": {
    "preinstall": "yarn clean",
    "clean": "(rm -rf interface/*) & (rm -f bin/gmc/*)",
    "copy": "(cp -R node_modules/interface .) & ( cp node_modules/gmc/bin/gmc bin/gmc/)",
    "postinstall": "yarn copy && yarn test-gmc && yarn build",
    "build": "yarn build-interface && yarn test-interface",
    "build-interface": "cd interface && ../node_modules/.bin/polymer install --quiet",
    "test-gmc": "bin/gmc/gmc version",
    "test-interface": "ls interface | grep bower_components",
    "start": "yarn nwb nwbuild --run .",
    "test": "echo 'Still needs unit tests.'"
  },
  "devDependencies": {
    "interface": "github:etaletai13/interface",
    "gmc": "github:etaletai13/gmc",
    "polymer-cli": "1.7.1"
  }
}
